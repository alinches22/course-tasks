{
  "name": "@tradeversus/shared",
  "version": "0.1.0",
  "private": true,
  "description": "TradeVersus Shared - Common types, utilities, and constants",
  "author": "TradeVersus Team",
  "license": "UNLICENSED",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./types": {
      "import": "./src/types/index.ts",
      "require": "./src/types/index.ts",
      "types": "./src/types/index.ts"
    },
    "./enums": {
      "import": "./src/enums/index.ts",
      "require": "./src/enums/index.ts",
      "types": "./src/enums/index.ts"
    },
    "./constants": {
      "import": "./src/constants/index.ts",
      "require": "./src/constants/index.ts",
      "types": "./src/constants/index.ts"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "require": "./src/utils/index.ts",
      "types": "./src/utils/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "clean": "rimraf dist node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0"
  }
}
